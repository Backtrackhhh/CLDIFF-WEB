<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>ServletMain</servlet-name>
        <servlet-class>edu.fdu.se.web.test.FileServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ProxyServlet</servlet-name>
        <servlet-class>edu.fdu.se.web.test.ProxyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletMain</servlet-name>
        <url-pattern>/getfile</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ProxyServlet</servlet-name>
        <url-pattern>/getfiles</url-pattern>
    </servlet-mapping>



    <!--<servlet-mapping>-->
    <!--<servlet-name>TestServlet</servlet-name>-->
    <!--<url-pattern>/</url-pattern>-->
    <!--</servlet-mapping>-->
</web-app>


